{
  "name": "@xpack-dev-tools/qemu-arm",
  "version": "2.8.0-12.1",
  "description": "A binary xPack with QEMU Arm executables",
  "main": "",
  "scripts": {
    "npm-install": "npm install",
    "pack": "npm pack",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "test": "echo \"Error: no test specified\" && exit 1",
    "liquidjs": "liquidjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpack-dev-tool/qemu-arm-xpack.git"
  },
  "bugs": {
    "url": "https://github.com/xpack-dev-tool/qemu-arm-xpack/issues/"
  },
  "homepage": "https://github.com/xpack-dev-tool/qemu-arm-xpack#readme",
  "keywords": [
    "xpack",
    "qemu",
    "arm"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "xpack": {
    "minimumXpmRequired": "0.10.2",
    "binaries": {
      "destination": "./.content",
      "baseUrl": "https://github.com/xpack-dev-tools/qemu-arm-xpack/releases/download/v2.8.0-12",
      "skip": 1,
      "platforms": {
        "darwin-x64": {
          "fileName": "xpack-qemu-arm-2.8.0-12-darwin-x64.tar.gz",
          "sha256": "1269339202b04fc765a7b1c37e0f46b0ecba5b880f26a43c7aa6a29f905e2edf"
        },
        "linux-arm64": {
          "fileName": "xpack-qemu-arm-2.8.0-12-linux-arm64.tar.gz",
          "sha256": "57c611d2469c227c71870a1ce5e0d35d88c0962795ea1a435a277db5f9fca79e"
        },
        "linux-arm": {
          "fileName": "xpack-qemu-arm-2.8.0-12-linux-arm.tar.gz",
          "sha256": "fa0a05ff7a31da9fb0228db4b7ee530e55d1bfe69bb1131a39f1d9a434bdcc91"
        },
        "linux-ia32": {
          "fileName": "xpack-qemu-arm-2.8.0-12-linux-ia32.tar.gz",
          "sha256": "285553bb38785e8e070b610b4b701886e6456acc38b23d8b3a40563a72ee3513"
        },
        "linux-x64": {
          "fileName": "xpack-qemu-arm-2.8.0-12-linux-x64.tar.gz",
          "sha256": "834dd2e9ade20e23f8639cf42e6dc61940f350148e858bb1f22c2a43b398e280"
        },
        "win32-ia32": {
          "fileName": "xpack-qemu-arm-2.8.0-12-win32-ia32.zip",
          "sha256": "b78b3336ee75a561b70d0ba8f5da6fb34088cdc7cffd6a6e51a16c5d2fa1fcca"
        },
        "win32-x64": {
          "fileName": "xpack-qemu-arm-2.8.0-12-win32-x64.zip",
          "sha256": "9608c3aae54e379d3450955f3e9008f28dbbfe7add2f563c595f443bf270cc4d"
        }
      }
    },
    "bin": {
      "qemu-system-gnuarmeclipse": "./.content/bin/qemu-system-gnuarmeclipse"
    },
    "properties": {
      "appName": "QEMU",
      "appLCName": "qemu-arm"
    },
    "actions": {
      "generate-workflows": [
        "mkdir -p .github/workflows/",
        "cp scripts/helper/templates/body-github-pre-releases-test.md .github/workflows/",
        "cat scripts/helper/templates/build-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\", \"APP_LC_NAME\": \"{{ properties.appLCName }}\" }' > .github/workflows/build.yml",
        "cat scripts/helper/templates/publish-release-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\", \"APP_LC_NAME\": \"{{ properties.appLCName }}\" }' > .github/workflows/publish-release.yml",
        "cat scripts/helper/templates/test-prime-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\", \"APP_LC_NAME\": \"{{ properties.appLCName }}\" }' > .github/workflows/test-prime.yml",
        "cat scripts/helper/templates/test-docker-linux-intel-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\" }' > .github/workflows/test-docker-linux-intel.yml",
        "cat scripts/helper/templates/test-docker-linux-arm-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\" }' > .github/workflows/test-docker-linux-arm.yml",
        "cat scripts/helper/templates/test-xpm-liquid.yml | liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\", \"APP_LC_NAME\": \"{{ properties.appLCName }}\" }' > .github/workflows/test-xpm.yml"
        ],
      "trigger-workflow-build": "bash scripts/helper/trigger-workflow-build.sh",
      "trigger-workflow-test-prime": "bash scripts/helper/tests/trigger-workflow-test-prime.sh",
      "trigger-workflow-test-docker-linux-intel": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-intel.sh",
      "trigger-workflow-test-docker-linux-arm": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-arm.sh",
      "trigger-travis-macos": "bash scripts/helper/tests/trigger-travis-macos.sh",
      "trigger-workflow-publish-release": "bash scripts/helper/trigger-workflow-publish-release.sh",
      "generate-jekyll-post": "bash scripts/helper/generate-jekyll-post.sh",
      "update-package-binaries": "bash scripts/helper/update-package-binaries.sh",
      "trigger-workflow-test-xpm": "bash scripts/helper/tests/trigger-workflow-test-xpm.sh"
    }
  },
  "bundleDependencies": [],
  "devDependencies": {
    "liquidjs": "^9.25.1"
  }
}
